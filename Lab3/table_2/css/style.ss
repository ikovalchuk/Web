.title {
 font-family: Gill Sans Extrabold, sans-serif;
 background-color: rgb(129,180,34);
 color: white;
 text-align: center;
}

.table {
 width: 500px;
 border-collapse: collapse;
}

tr:nth-child(even) {
 background-color:  rgb(229,255,184);
}

 tr:nth-child(odd) {
  background-color: rgb(239,255,211);
}

td {
 border: 2px solid black;
}
